@model Category

<div class="text-center">
<h1>@Model.Name</h1>
</div>

<ul>
    @foreach (Association a in Model.AssociatedProducts)
    {
        <li >@a.Product.Name</li>
    }
</ul>

@{
    Association association = new Association();
    association.CategoryId = Model.CategoryId;
}
@await Html.PartialAsync("_AddProducts", association)




